[user]
  name = shikakun
  email = k@u-n.jp
[push]
  default = simple
[color]
  ui = auto
  diff = auto
  status = auto
  branch = auto
[alias]
  a = add
  c = commit -m
  ci = commit -m
  ca = commit --amend
  co = checkout
  d = diff
  dd = diff --cached
  p = push origin
  pl = pull origin
  s = status -sb
  st = status -sb
  br = branch
  brd = branch -d
  brdd = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
[core]
  editor = vim
  excludesfile = ~/.gitignore
